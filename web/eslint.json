{
  "extends": [
    "eslint:recommended"
  , "google"
  ]
,
  "parserOptions": {
    "ecmaVersion": 2017
  , "sourceType" : "module"
  }
, "rules": {

    "no-undef"                 : "error"
  , "no-unused-vars"           : "error"

  , "no-throw-literal"         : "warn"
  , "no-unexpected-multiline"  : "warn"
  , "spaced-comment"           : "warn"
  , "no-constant-condition"    : "warn"

  , "array-bracket-spacing"    : "off"
  , "block-spacing"            : "off"
  , "brace-style"              : "off"
  , "camelcase"                : "off"
  , "comma-dangle"             : "off"
  , "comma-spacing"            : "off"
  , "comma-style"              : "off"
  , "computed-property-spacing": "off"
  , "curly"                    : "off"
  , "func-call-spacing"        : "off"
  , "indent"                   : "off"
  , "key-spacing"              : "off"
  , "max-len"                  : "off"
  , "no-multi-spaces"          : "off"
  , "object-curly-spacing"     : "off"
  , "padded-blocks"            : "off"
  , "quotes"                   : "off"
  , "require-jsdoc"            : "off"
  , "semi"                     : "off"

  }
, "globals": {

    "console"           : "readonly"
  , "document"          : "readonly"
  , "localStorage"      : "readonly"
  , "navigator"         : "readonly"
  , "window"            : "readonly"

  , "glMatrix"          : "readonly"
  , "proto"             : "readonly"

  , "module"            : "readonly"
  , "require"           : "readonly"

  , "FileReader"        : "readonly"
  , "Float32Array"      : "readonly"
  , "Map"               : "readonly"
  , "Set"               : "readonly"
  , "Uint8Array"        : "readonly"
  , "Uint16Array"       : "readonly"
  , "Uint32Array"       : "readonly"
  , "VRFrameData"       : "readonly"
  , "WebSocket"         : "readonly"
  , "WebVRPolyfill"     : "readonly"

  , "uiAddress"         : "readonly"
  , "uiBody"            : "readonly"
  , "uiCanvas"          : "readonly"
  , "uiConnector"       : "readonly"
  , "uiDownload"        : "readonly"
  , "uiEyeSeparationX"  : "readonly"
  , "uiEyeSeparationY"  : "readonly"
  , "uiEyeSeparationZ"  : "readonly"
  , "uiFarPlane"        : "readonly"
  , "uiKeyboard"        : "readonly"
  , "uiLowerLeftX"      : "readonly"
  , "uiLowerLeftY"      : "readonly"
  , "uiLowerLeftZ"      : "readonly"
  , "uiLowerRightX"     : "readonly"
  , "uiLowerRightY"     : "readonly"
  , "uiLowerRightZ"     : "readonly"
  , "uiMono"            : "readonly"
  , "uiNearPlane"       : "readonly"
  , "uiStereo"          : "readonly"
  , "uiTexture"         : "readonly"
  , "uiToolOrientationX": "readonly"
  , "uiToolOrientationY": "readonly"
  , "uiToolOrientationZ": "readonly"
  , "uiToolPositionX"   : "readonly"
  , "uiToolPositionY"   : "readonly"
  , "uiToolPositionZ"   : "readonly"
  , "uiUpperLeftX"      : "readonly"
  , "uiUpperLeftY"      : "readonly"
  , "uiUpperLeftZ"      : "readonly"
  , "uiVR"              : "readonly"
  , "uiViewOrientationX": "readonly"
  , "uiViewOrientationY": "readonly"
  , "uiViewOrientationZ": "readonly"
  , "uiViewPositionX"   : "readonly"
  , "uiViewPositionY"   : "readonly"
  , "uiViewPositionZ"   : "readonly"
  , "uiVisualize"       : "readonly"

  }
}
